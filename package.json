{
  "name": "cogs-kings-of-war",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --style=compressed",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:arc": "node ./dev sandbox",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env SKIP_EMAIL=\"skip\" remix watch",
    "format": "prettier --write .",
    "generate:css": "sass -Inode_modules/foundation-sites/scss app/styles",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "test": "vitest",
    "test:e2e:dev": "cross-env SKIP_EMAIL=\"skip\" start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "test:e2e:run": "cross-env PORT=8811 SKIP_EMAIL=skip start-server-and-test dev http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/server/index.js",
    "/public/build"
  ],
  "dependencies": {
    "@architect/architect": "^10.16.1",
    "@architect/functions": "^7.0.0",
    "@aws-sdk/client-s3": "^3.449.0",
    "@aws-sdk/client-ses": "^3.449.0",
    "@hapi/iron": "^7.0.1",
    "@remix-run/architect": "^1.19.3",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/server-runtime": "^1.19.3",
    "bcryptjs": "^2.4.3",
    "cuid": "^3.0.0",
    "feather-icons": "^4.29.1",
    "isbot": "^3.7.0",
    "markdown-it": "^13.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.11.0",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/architect__functions": "^3.13.11",
    "@types/bcryptjs": "2.4.6",
    "@types/eslint": "^8.44.7",
    "@types/markdown-it": "^13.0.6",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6r",
    "c8": "^8.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.5.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "foundation-sites": "^6.8.1",
    "happy-dom": "^12.10.3",
    "mock-aws-s3": "^4.0.2",
    "msw": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18"
  }
}
